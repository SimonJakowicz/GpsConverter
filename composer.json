    "keywords": ["gps", "latitude", "longitude", "converter"],
    "homepage": "https://github.com/SimonJakowicz/GpsConverter.git",
    "license": "MIT",
    "authors": [
        {
            "name": "Simon Jakowicz",
            "email": "simon@jakowicz.com",
            "homepage": "http://www.jakowicz.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.0"
    },
    "require-dev": {
        "phpmd/phpmd": "1.5.0",
        "sebastian/phpcpd" : "2.0.*",
        "squizlabs/php_codesniffer": "1.*",
        "pdepend/pdepend" : "1.1.1"
    },
    "autoload": {
        "psr-4": {
